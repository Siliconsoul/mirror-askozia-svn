moh_extract() {
	echo "Extracting $1 ($taropt) ... "
	mkdir $pkg-$ver
	tar -v $taropt $1 -C $pkg-$ver | sed 's,^,$pkg-$ver/,' > untar.txt
}

moh_main() {

	cp -p macroform-* manolo_camp-* reno_project-* $root/offload/asterisk/moh/
	echo "opsound music-on-hold full set copied into build"

}

custextract="moh_extract"
custmain="moh_main"
