prompts_extract() {
	echo "Extracting $1 ($taropt) ... "
	mkdir $pkg-$ver
	tar -v $taropt $1 -C $pkg-$ver | sed 's,^,$pkg-$ver/,' > untar.txt
}

prompts_main() {

	mkdir -p $root/asterisk/sounds/$prompt_language
	cp -pR * $root/asterisk/sounds/$prompt_language
	echo "$prompt_language prompts copied into build"

}

custextract="prompts_extract"
custmain="prompts_main"
