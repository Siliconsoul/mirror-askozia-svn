prompts_main() {

	mkdir stage
	cp -p es/* stage

	mkdir stage/dictate
	cp -p dictate/es/* stage/dictate

	mkdir stage/digits
	cp -p digits/es/* stage/digits

	mkdir stage/followme
	cp -p followme/es/* stage/followme

	mkdir stage/letters
	cp -p letters/es/* stage/letters

	mkdir stage/phonetic
	cp -p phonetic/es/* stage/phonetic

	mkdir stage/silence
	cp -p silence/es/* stage/silence

	mkdir -p $root/offload/asterisk/sounds/es-es
	cp -pRv stage/* $root/offload/asterisk/sounds/es-es

}

custmain="prompts_main"
