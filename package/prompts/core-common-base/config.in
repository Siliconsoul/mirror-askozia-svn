prompts_extract() {
	echo "Extracting $1 ($taropt) ... "
	mkdir $pkg-$ver
	cd $pkg-$ver
	tar -v $taropt $1 --wildcards --no-anchored 'agent-pass.*' 'auth-thankyou.*' 'auth-incorrect.*' 'conf-onlyperson.*' 'conf-getpin.*' 'conf-invalidpin.*' 'conf-kicked.*' 'pbx-transfer.*' 'pbx-invalid.*' 'pbx-invalidpark.*' 'vm-intro.*' 'vm-theperson.*' 'vm-isunavail.*' 'vm-isonphone.*' 'vm-goodbye.*' 'vm-password.*' 'digits/0.*' 'digits/1.*' 'digits/2.*' 'digits/3.*' 'digits/4.*' 'digits/5.*' 'digits/6.*' 'digits/7.*' 'digits/8.*' 'digits/9.*' | sed 's,^,$pkg-$ver/,' > ../untar.txt
	cd ..
}

prompts_main() {

	mkdir -p $root/offload/asterisk/sounds/$prompt_language
	cp -pR * $root/offload/asterisk/sounds/$prompt_language
	echo "$prompt_language prompts base set copied into build"

}

custextract="prompts_extract"
custmain="prompts_main"
